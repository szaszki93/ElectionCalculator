<UserControl x:Class="ElectionCalculatorView.View.ResultView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:ElectionCalculatorView.View"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <Button Command ="{Binding ShowGraphCmd}" Height="23" Width="80">Show graph</Button>
        <Button Command ="{Binding LogoutCmd}" Height="23" Width="80">Logout</Button>
    </StackPanel>
</UserControl>